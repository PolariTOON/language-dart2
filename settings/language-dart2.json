{
	".source.dart": {
		"editor": {
			"commentStart": "/* ",
			"commentEnd": " */",
			"foldEndPattern": "^\\s*(\\)|\\]|\\})",
			"increaseIndentPattern": "(\\(|\\[|\\{)\\s*$",
			"decreaseIndentPattern": "^\\s*(\\)|\\]|\\})"
		}
	},
	"comments": {
		"blockComment": [
			"/*",
			"*/"
		]
	},
	"folding": {
		"markers": {
			"start": "(\\(|\\[|\\{)\\s*$",
			"end": "^\\s*(\\)|\\]|\\})"
		}
	},
	"indentationRules": {
		"increaseIndentPattern": "(\\(|\\[|\\{)\\s*$",
		"decreaseIndentPattern": "^\\s*(\\)|\\]|\\})"
	}
}
